{
    "name": "rezzza/processone-bundle",
    "description": "ProcessOne bundle for Symfony2 projects",
    "keywords": ["processone", "notification", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rezzza/ProcessOneBundle",
    "license": "MIT",
    "authors": [{
        "name":     "Community contributors",
        "homepage": "https://github.com/rezzza/ProcessOneBundle/contributors"
    }],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "atoum/atoum": "*",
        "guzzle/guzzle": "3.*"
    },
    "suggests": {
        "guzzle": "use guzzle transport"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "autoload":     {
        "psr-0": { 
            "Rezzza\\ProcessOneBundle": "" 
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "target-dir": "Rezzza/ProcessOneBundle"
}
